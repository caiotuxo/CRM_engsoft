<h1>Listing marketing_actions</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Type</th>
    <th>Value</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @marketing_actions.each do |marketing_action| %>
  <tr>
    <td><%= marketing_action.name %></td>
    <td><%= marketing_action.description %></td>
    <td><%= marketing_action.type %></td>
    <td><%= marketing_action.value %></td>
    <td><%= link_to 'Show', marketing_action %></td>
    <td><%= link_to 'Edit', edit_marketing_action_path(marketing_action) %></td>
    <td><%= link_to 'Destroy', marketing_action, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

