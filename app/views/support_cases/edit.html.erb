<h1>Edicao de Chamado</h1>
<%= form_for @support_case, :validate => true, :html => {:class => "form-horizontal"} do |f|%>
    <%= render(:partial => 'form',:locals => {:f => f}) %>
    <div class="control-group">
      <div class="controls">
        <%= f.submit "Salvar chamado",:class => "btn btn-small dark-blue"%>
      </div>
    </div>
<%end%>

<%= link_to 'Voltar', support_cases_path,:class => "btn btn-small dark-blue"  %>
