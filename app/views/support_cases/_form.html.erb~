<%= form_for(@support_case) do |f| %>
  <% if @support_case.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@support_case.errors.count, "error") %> prohibited this support_case from being saved:</h2>

      <ul>
      <% @support_case.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :creationDate %><br />
    <%= f.text_field :creationDate %>
  </div>
  <div class="field">
    <%= f.label :caseCode %><br />
    <%= f.text_field :caseCode %>
  </div>
  <div class="field">
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </div>
  <div class="field">
    <%= f.label :type %><br />
    <%= f.text_field :type %>
  </div>
  <div class="field">
    <%= f.label :status %><br />
    <%= f.text_field :status %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
